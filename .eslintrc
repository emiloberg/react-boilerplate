{
  "ecmaFeatures": {
    "jsx": true,
    "modules": true
  },
  "env": {
    "browser": true,
    "node": true
  },
  "parser": "babel-eslint",
  "rules": {
    "strict": [2, "never"],
    "quotes": [2, "single", "avoid-escape"],
    "no-use-before-define": 0,
    "no-underscore-dangle": 0,
    "eol-last": 2,
    "no-trailing-spaces": 2,
    "no-unused-vars": 2,
    "key-spacing": 0,
    "react/jsx-uses-react": 2,
    "react/jsx-uses-vars": 2,
    "react/react-in-jsx-scope": 2
  },
  "plugins": [
    "react"
  ]
}
