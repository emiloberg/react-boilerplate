{
  "name": "react-boilerplate",
  "version": "1.0.0",
  "description": "Minimal React boilerplate",
  "scripts": {
    "start": "node dev-server.js",
    "lint": "eslint src",
    "test": "mocha --require babel-core/register --recursive \"test/**/*.@(js|jsx)\"",
    "test:watch": "npm test -- --watch"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/emiloberg/react-boilerplate.git"
  },
  "keywords": [
    "react",
    "jsx",
    "boilerplate",
    "es6",
    "es2015"
  ],
  "author": "Emil Oberg",
  "license": "WTFPL",
  "bugs": {
    "url": "https://github.com/emiloberg/react-boilerplate/issues"
  },
  "homepage": "https://github.com/emiloberg/react-boilerplate",
  "devDependencies": {
    "babel-core": "^6.0.20",
    "babel-eslint": "^4.1.3",
    "babel-loader": "^6.0.1",
    "babel-preset-es2015": "^6.0.15",
    "babel-preset-react": "^6.0.15",
    "babel-preset-stage-0": "^6.0.15",
    "eslint-plugin-react": "^3.6.2",
    "mocha": "^2.3.4",
    "react-hot-loader": "^1.3.0",
    "webpack": "^1.12.2",
    "webpack-dev-server": "^1.12.1"
  },
  "dependencies": {
    "react": "^0.14.0",
    "react-dom": "^0.14.0"
  }
}
